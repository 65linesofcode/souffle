
.decl gen(x: number)
gen(1).
gen(x+1) :- gen(x), !lim(x).

.decl gen2(x: number)
gen2(1).
gen2(x+1) :- gen2(x), !lim(x).

.decl lim(x : number)
lim(1000).

.decl transform(x : number, y : number) output
transform(x, y) :- transform(y, x).
transform(x, z) :- transform(x, y), transform(y, z).
transform(x, y) :- gen(x), gen2(y).
